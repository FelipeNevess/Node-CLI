{
  "name": "node-cli-v2",
  "version": "1.1.0",
  "main": "./dist/run.js",
  "bin": {
    "node-app": "./dist/run.js"
  },
  "author": "felipenevess <felipe012neves@gmail.com>",
  "license": "MIT",
  "contributors": [
    {
      "name": "fedolfo",
      "email": "filipecandido123@hotmail.com",
      "url": "https://github.com/Fedolfo"
    }
  ],
  "bugs": "https://github.com/FelipeNevess/Node-CLI",
  "keywords": [
    "cli",
    "create-node-app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FelipeNevess/Node-CLI"
  },
  "description": "A CLI for creating NodeJs projects",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn app/run.ts",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "local": "sudo npm i -g || npm i -g && node-app",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "build": "tsc -p ."
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/jest": "^28.1.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.2",
    "@types/prompts": "2.0.14",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "figlet": "^1.5.2",
    "prompts": "^2.4.2"
  }
}